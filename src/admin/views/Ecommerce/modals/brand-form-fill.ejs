<div class="modal fade" id="brandFormFillModal" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-body">
        <form id="brandFormFill" action="<%= api %>/brand/add" method="POST">
          <div class="mb-3 row">
            <label for="nameBrand" class="col-md-3 col-form-label">Name</label>
            <div class="col-md-9">
              <input class="form-control" type="text" name="name" id="nameBrand" required />
            </div>
          </div>
        </form>
        <div class="modal-footer">
          <button type="button" class="btn btn-light" data-bs-dismiss="modal">Close</button>
          <button type="submit" form="brandFormFill" class="btn btn-primary">Save changes</button>
        </div>
      </div>
    </div>
    <!-- /.modal-content -->
  </div>
  <!-- /.modal-dialog -->
</div>
<!-- /.modal -->
