<div class="modal fade" id="categoryFormFillModal" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-body">
        <form id="categoryFormFill" action="<%= api %>/category/add" method="POST">
          <div class="mb-3 row">
            <label for="userUserName" class="col-md-3 col-form-label">Name</label>
            <div class="col-md-9">
              <input class="form-control" type="text" name="name" id="nameCategory" required />
            </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-light" data-bs-dismiss="modal">Close</button>
            <button type="submit" form="categoryFormFill" class="btn btn-primary">Save</button>
          </div>
        </form>
      </div>
    </div>
    <!-- /.modal-content -->
  </div>
  <!-- /.modal-dialog -->
</div>
<!-- /.modal -->
